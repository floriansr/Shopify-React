{
  "env": {
    "browser": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": [
          "node_modules",
          "src" // replace with your app-module-path directory
        ]
      }
    }
  },
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "consistent-return": [
      "off"
    ],
    "react/prop-types": [
      "off"
    ],
    "react/jsx-props-no-spreading": [
      "off"
    ],
    "no-console": [
      "off"
    ],
    "no-underscore-dangle": [
      "off"
    ],
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true
      }
    ],
    "jsx-a11y/label-has-associated-control": [
      "warn",
      {
        "labelComponents": [],
        "labelAttributes": [],
        "controlComponents": [],
        "assert": "nesting"
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "tabWidth": 2
      }
    ]
  }
}